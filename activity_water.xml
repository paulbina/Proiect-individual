<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".water">

    <Button
        android:id="@+id/water1"
        android:layout_width="280dp"
        android:layout_height="65dp"
        android:text="Water1        4lei"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/water2"
        android:layout_width="280dp"
        android:layout_height="65dp"
        android:text="Water2        4lei"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/water1" />

    <Button
        android:id="@+id/water3"
        android:layout_width="280dp"
        android:layout_height="65dp"
        android:text="Water3        8lei"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/water2" />
</androidx.constraintlayout.widget.ConstraintLayout>